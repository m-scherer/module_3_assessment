<h1><%= @stores.count %> Total Stores</h1>
<% @stores.each do |store| %>
<ul id="store">
  <li> <%= link_to "Name: #{store.name}", store_path(id: store.id), id: "name" %> </li>
  <li>City: <%= store.city %></li>
  <li>Distance: <%= store.distance %></li>
  <li>Phone: <%= store.phone %></li>
  <li>Type: <%= store.type %></li>
</ul>
<% end %>
